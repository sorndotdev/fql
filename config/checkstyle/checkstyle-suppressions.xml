<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <!-- generic -->
    <suppress files=".*TestData\.java" checks=".*"/>
    <suppress files=".*Test\.java" checks=".*"/>
    <suppress files=".*Defs\.java" checks=".*"/>
    <suppress files=".*[\\/]generated[\\/].*" checks=".*"/>
    <suppress files=".*[\\/]generated-sources[\\/].*" checks=".*"/>

    <!-- specific -->
    <suppress files="DataPoint.java" checks="NeedBraces"/> <!-- reason: equals() -->
    <suppress files="DataPoint.java" checks="NPathComplexity"/> <!-- reason: equals() -->
    <suppress files="FiscalYear.java" checks="NeedBraces"/> <!-- reason: equals() -->
    <suppress files="FiscalQuarter.java" checks="NeedBraces"/> <!-- reason: equals() -->
    <suppress files="FiscalPeriod.java" checks="NeedBraces"/> <!-- reason: equals() -->
    <suppress files="FiscalPeriod.java" checks="MagicNumber"/> <!-- reason: hashCode() -->
    <suppress files="FiscalPeriodRange.java" checks="NeedBraces"/> <!-- reason: equals() -->
    <suppress files="FiscalPeriodRange.java" checks="MagicNumber"/> <!-- reason: hashCode() -->
    <suppress files="Optionality.java" checks="NeedBraces"/> <!-- reason: optionalComparator() -->
</suppressions>